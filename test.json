{  
   "$schema":"http://schema.management.azure.com/schemas/2014-04-01-preview/VM_IP.json",
   "contentVersion":"1.0.0.0",
   "parameters":{  
      "vmName":{  
         "type":"string"
      },
      "vmSize":{  
         "type":"string"
      },
      "adminUserName":{  
         "type":"securestring"
      },
      "adminPassword":{  
         "type":"securestring"
      },
      "networkInterfaceId":{  
         "type":"string"
      },
      "location":{
         "type":"string"
      }
   },
   "resources":[  
      {  
         "apiVersion":"2015-06-15",
         "properties":{  
            "hardwareProfile":{  
               "vmSize":"[parameters('vmSize')]"
            },
            "storageProfile":{  
               "osDisk":{  
                  "osType":"Linux",
                  "name":"RelayMarketplace-osDisk.1182d0e0-926e-4754-816b-5460b567da1c.vhd",
                  "createOption":"FromImage",
                  "image":{  
                     "uri":"https://relaymarketplace5831.blob.core.windows.net/system/Microsoft.Compute/Images/vhds/RelayMarketplace-osDisk.1182d0e0-926e-4754-816b-5460b567da1c.vhd"
                  },
                  "vhd":{  
                     "uri":"https://relaymarketplace5831.blob.core.windows.net/vmcontainerf13a0d10-4d5f-40cd-afaf-c978cd435d97/osDisk.f13a0d10-4d5f-40cd-afaf-c978cd435d97.vhd"
                  },
                  "caching":"ReadWrite"
               }
            },
            "osProfile":{  
               "computerName":"[parameters('vmName')]",
               "adminUsername":"[parameters('adminUsername')]",
               "adminPassword":"[parameters('adminPassword')]"
            },
            "networkProfile":{  
               "networkInterfaces":[  
                  {  
                     "id":"[parameters('networkInterfaceId')]"
                  }
               ]
            },
            "diagnosticsProfile":{  
               "bootDiagnostics":{  
                  "enabled":true,
                  "storageUri":"https://relaymarketplace5831.blob.core.windows.net/"
               }
            },
            "provisioningState":0
         },
         "name":"[parameters('vmName')]",
         "type":"Microsoft.Compute/virtualMachines",
         "location":"[parameters('location')]",
         "size":"[parameters('vmSize')]"
      }
   ]
}
